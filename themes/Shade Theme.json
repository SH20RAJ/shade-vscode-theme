		{
			"name": "Embedded",
			"scope": [
				"punctuation.section.embedded",
				"variable.language.embedded",
				"meta.tag.metadata.embedded",
				"meta.tag.block.any.embedded",
				"meta.return-type variable.language.embedded",
				"meta.block punctuation.section.embedded",
				"meta.block variable.language.embedded",
				"meta.block meta.tag.metadata.embedded",
				"meta.block meta.tag.block.any.embedded",
				"meta.block meta.return-type variable.language.embedded"
			],
			"settings": {
				"foreground": "#f8f8f2"
			}
		}
