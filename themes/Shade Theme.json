	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#6272a4"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#f8f8f2"
			}
		},
		{
			"name": "String constant",
			"scope": [
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#50fa7b"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#f8f8f2"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#ff5555"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage",
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.logical.python"
			],
			"settings": {
				"foreground": "#ff79c6"
			}
		},
