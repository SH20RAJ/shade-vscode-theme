				"keyword.operator.arithmetic",
				"keyword.operator.logical",
				"meta.preprocessor.c.include"
			],
			"settings": {
				"foreground": "#ff79c6"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"storage",
				"storage.type",
				"storage.modifier",
